{
    "type": "module",
    "name": "@omuchatjs/chat",
    "version": "0.1.12",
    "repository": "https://github.com/OMUCHAT/omuchat/",
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        ".": {
            "import": "./built/esm/index.js",
            "types": "./built/dts/index.d.ts"
        },
        "./*": {
            "import": "./built/esm/*",
            "types": "./built/dts/*"
        }
    },
    "scripts": {
        "ts": "npm run ts-esm && npm run ts-dts",
        "ts-esm": "tsc --outDir built/esm",
        "ts-dts": "tsc --outDir built/dts --declaration true --emitDeclarationOnly true --declarationMap true",
        "watch": "nodemon -w src -e ts,json --exec \"npm run ts\"",
        "dev": "npm run watch",
        "build": "npm run ts",
        "lint": "eslint . --ext .js,.ts",
        "fix": "eslint . --ext .js,.ts --fix"
    },
    "devDependencies": {
        "@omuchatjs/omu": "workspace:*",
        "@types/node": "^20.12.3",
        "@types/websocket": "1.0.10",
        "@types/ws": "8.5.10",
        "@typescript-eslint/eslint-plugin": "^7.5.0",
        "@typescript-eslint/parser": "^7.5.0",
        "axios": "^1.6.8",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-svelte": "^2.35.1",
        "eslint-plugin-unused-imports": "^3.1.0",
        "nodemon": "^3.1.0",
        "ts-node": "10.9.2",
        "typescript": "^5.4.3",
        "websocket": "1.0.34",
        "ws": "8.16.0"
    },
    "dependencies": {
        "@omuchatjs/omu": "workspace:*"
    },
    "gitHead": "04559d5aec52117d707e0d554ee429b858da522a"
}
