{
    "type": "module",
    "name": "@omujs/chat",
    "version": "0.9.50",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./*": {
            "import": "./dist/*/index.js",
            "types": "./dist/*/index.d.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "bun ../../scripts/package-helper.ts --glob --entrypoint \"src/**/*.ts\" --out \"./dist\" --root \"src/\"",
        "dev": "bun run watch",
        "watch": "bun ../../scripts/package-helper.ts --glob --entrypoint \"src/**/*.ts\" --out \"./dist\" --root \"src/\" --watch"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/OMUAPPS/omuapps"
    },
    "peerDependencies": {
        "@omujs/omu": "workspace:*"
    },
    "gitHead": "04559d5aec52117d707e0d554ee429b858da522a"
}
