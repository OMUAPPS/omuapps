{
    "type": "module",
    "name": "@omujs/omu",
    "version": "0.6.20",
    "exports": {
        ".": {
            "import": "./built/esm/index.js",
            "types": "./built/dts/index.d.ts"
        },
        "./*": {
            "import": "./built/esm/*",
            "types": "./built/dts/*"
        }
    },
    "scripts": {
        "build": "pnpm run ts",
        "dev": "pnpm run watch",
        "format": "eslint . --ext .js,.ts --fix",
        "lint": "eslint . --ext .js,.ts",
        "ts": "pnpm run ts-esm && pnpm run ts-dts",
        "ts-dts": "tsc --outDir built/dts --declaration true --emitDeclarationOnly true --declarationMap true",
        "ts-esm": "tsc --outDir built/esm",
        "watch": "nodemon -w src -e ts,json --exec \"pnpm --workspace-root build --only @omujs/omu\""
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/OMUAPPS/omuapps"
    },
    "devDependencies": {
        "eslint": "^9.8.0"
    },
    "gitHead": "04559d5aec52117d707e0d554ee429b858da522a",
    "srcHash": "8d57d6f33bc51e0fffb87e7e37f05407"
}
