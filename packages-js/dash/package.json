{
    "private": true,
    "type": "module",
    "name": "dash",
    "version": "0.0.0",
    "scripts": {
        "build": "pnpm run build-pre && tauri build",
        "build-pre": "node ./scripts/build-pre.mjs",
        "dev": "tauri dev",
        "dev-release": "tauri dev --release",
        "prepare": "svelte-kit sync",
        "tauri": "tauri",
        "ui:build": "vite build",
        "ui:check": "svelte-check --tsconfig ./tsconfig.json",
        "ui:check-watch": "pnpm run ui:check --watch",
        "ui:dev": "vite --host 0.0.0.0 --port 26420",
        "ui:preview": "vite preview",
        "lint": "eslint .",
        "fix": "eslint . --fix"
    },
    "devDependencies": {
        "@fontsource/noto-sans-jp": "5.0.19",
        "@omujs/chat": "workspace:*",
        "@omujs/eslint-config": "workspace:*",
        "@omujs/i18n": "workspace:*",
        "@omujs/omu": "workspace:*",
        "@omujs/ui": "workspace:*",
        "@sveltejs/adapter-static": "3.0.2",
        "@sveltejs/kit": "2.5.20",
        "@sveltejs/vite-plugin-svelte": "3.1.1",
        "@tauri-apps/api": "1.6.0",
        "@tauri-apps/cli": "1.6.0",
        "@tsconfig/svelte": "5.0.4",
        "@types/eslint": "^9.6.0",
        "eslint": "^9.9.1",
        "eslint-plugin-svelte": "^2.36.0",
        "esm-env": "1.0.0",
        "globals": "^15.0.0",
        "license-checker": "25.0.1",
        "sass": "1.77.8",
        "sugarss": "4.0.1",
        "svelte": "4.2.18",
        "svelte-check": "3.8.5",
        "svelte-markdown": "0.4.1",
        "tslib": "2.6.3",
        "typescript-eslint": "^8.0.0",
        "vite": "5.4.0",
        "vitest": "2.0.5"
    }
}
