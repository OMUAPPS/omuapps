{
    "private": true,
    "type": "module",
    "name": "dash",
    "version": "0.0.0",
    "scripts": {
        "build": "bun run build-pre && tauri build",
        "build-pre": "bun ./scripts/build-pre.ts",
        "dev": "tauri dev",
        "dev-release": "tauri dev --release --no-dev-server",
        "prepare": "bun x --bun svelte-kit sync",
        "tauri": "tauri",
        "ui:build": "bun x --bun vite build",
        "ui:check": "bun x --bun svelte-check --tsconfig ./tsconfig.json",
        "ui:check-watch": "bun run ui:check --watch",
        "ui:dev": "bun x --bun vite --host 0.0.0.0",
        "ui:preview": "bun x --bun vite preview"
    },
    "devDependencies": {
        "@fontsource/noto-sans-jp": "5.0.19",
        "@tabler/icons-webfont": "^3.19.0",
        "@omujs/chat": "workspace:*",
        "@omujs/i18n": "workspace:*",
        "@omujs/omu": "workspace:*",
        "@omujs/ui": "workspace:*",
        "@sveltejs/adapter-static": "3.0.2",
        "@sveltejs/kit": "2.5.20",
        "@sveltejs/vite-plugin-svelte": "3.1.1",
        "@tauri-apps/api": "2.2.0",
        "@tauri-apps/cli": "2.2.7",
        "@tauri-apps/plugin-process": "^2.2.0",
        "@tauri-apps/plugin-updater": "^2.5.0",
        "@tsconfig/svelte": "5.0.4",
        "@types/license-checker": "25.0.6",
        "esm-env": "1.0.0",
        "sass": "^1.86.0",
        "license-checker": "25.0.1",
        "sugarss": "4.0.1",
        "svelte": "4.2.18",
        "svelte-check": "3.8.5",
        "svelte-markdown": "0.4.1",
        "tslib": "2.6.3",
        "vite": "5.4.0",
        "vitest": "2.0.5"
    }
}
